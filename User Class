using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace pr
{
    class User
    {
        public Project project;
        private Task task;

        public void setProject(Project pro)
        {
            this.project = pro;
        }

        public void setTask(Task tsk)
        {
            this.task = tsk;
        }

        public Task getTask()
        {
            return this.task;
        }
    }
}
